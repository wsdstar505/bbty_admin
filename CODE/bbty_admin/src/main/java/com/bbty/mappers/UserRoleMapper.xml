<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >  
<mapper namespace="com.bbty.dao.UserRoleDao" >    
  
  <select id="getUserRoleByUserName" parameterType="string" resultType="string">
　　　SELECT T.ROLEID FROM T_USER_ROLE T,T_USER U WHERE T.EMPID=U.EMPID AND U.USERNAME=#{username}
　  </select>

</mapper>